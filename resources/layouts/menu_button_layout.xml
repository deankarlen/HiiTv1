<layout id="MenuButtonLayout">
    <button x="55" y="20" width="30" height="30" background="Gfx.COLOR_BLACK" behavior="onPreviousMode">
        <state id="stateDefault" bitmap="@Drawables.DefaultMinusButton" />
        <state id="stateHighlighted" bitmap="@Drawables.PressedMinusButton" />
        <state id="stateSelected" bitmap="@Drawables.PressedMinusButton" />
        <state id="stateDisabled" color="Gfx.COLOR_BLACK" />
    </button>
    <button x="115" y="20" width="30" height="30" background="Gfx.COLOR_BLACK" behavior="onNextMode">
        <state id="stateDefault" bitmap="@Drawables.DefaultPlusButton" />
        <state id="stateHighlighted" bitmap="@Drawables.PressedPlusButton" />
        <state id="stateSelected" bitmap="@Drawables.PressedPlusButton" />
        <state id="stateDisabled" color="Gfx.COLOR_BLACK" />
    </button>
    <button x="55" y="80" width="30" height="30" background="Gfx.COLOR_BLACK" behavior="onPreviousPage">
        <state id="stateDefault" bitmap="@Drawables.DefaultMinusButton" />
        <state id="stateHighlighted" bitmap="@Drawables.PressedMinusButton" />
        <state id="stateSelected" bitmap="@Drawables.PressedMinusButton" />
        <state id="stateDisabled" color="Gfx.COLOR_BLACK" />
    </button>
    <button x="115" y="80" width="30" height="30" background="Gfx.COLOR_BLACK" behavior="onNextPage">
        <state id="stateDefault" bitmap="@Drawables.DefaultPlusButton" />
        <state id="stateHighlighted" bitmap="@Drawables.PressedPlusButton" />
        <state id="stateSelected" bitmap="@Drawables.PressedPlusButton" />
        <state id="stateDisabled" color="Gfx.COLOR_BLACK" />
    </button>
    <button x="40" y="140" width="30" height="30" background="Gfx.COLOR_BLACK" behavior="onBack">
        <state id="stateDefault" bitmap="@Drawables.DefaultMinusButton" />
        <state id="stateHighlighted" bitmap="@Drawables.PressedMinusButton" />
        <state id="stateSelected" bitmap="@Drawables.PressedMinusButton" />
        <state id="stateDisabled" color="Gfx.COLOR_BLACK" />
    </button>
    <button x="115" y="140" width="30" height="30" background="Gfx.COLOR_BLACK" behavior="onSelect">
        <state id="stateDefault" bitmap="@Drawables.DefaultPlusButton" />
        <state id="stateHighlighted" bitmap="@Drawables.PressedPlusButton" />
        <state id="stateSelected" bitmap="@Drawables.PressedPlusButton" />
        <state id="stateDisabled" color="Gfx.COLOR_BLACK" />
    </button>
</layout>
